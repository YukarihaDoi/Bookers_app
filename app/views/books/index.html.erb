<p>Book was successfully destroyed.</p>
<h1>Books</h1>

 <% @books.each do|book|%>
 <p>Title</p>
 <span><%= book.title%></span>

 <p>Body</p>
 <span><%= book.body%></span>
 <%end%>

<h2>New book</h2>
<%= form_with model: @book do|f|%>

<h4>Title</h4>
 <%= f.text_field :title %>

 <h4>Body</h4>
 <%= f.text_area :body %>
 <br>
 <%= f.submit 'Create Book' %>
<%end%>
